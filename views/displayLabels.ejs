<html>
    <head>
        <title>Homepage</title>
        <link rel="stylesheet" type="text/css" href="/css/styles.css" />
    </head>
    <body>        
        <%- include('./sideBar'); %>
        <div class="rightContainer">
            <%- include('./header'); %>
            <div class="mainContainer">
                <div class="list">
                    <h2>All Labels</h2>
                    <% for(let i=0;i<allLabels.length ; i++) { %>
                        <a class="infoLink" href="/label/<%=allLabels[i].id%>"><h4><%=allLabels[i].labelname%></h4></a>
                    <%}%>
                </div>  
            </div>
        </div>
    </body>
</html>