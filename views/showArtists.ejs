<p><%= allArtists.length %> Artists</p>
<div class="mainContainer">

                
    <% for(let i=0;i<allArtists.length ; i++) { %>
        <div class="eachItem eachArtist">
            <div class="itemImage">
                <a href="/artist/<%=allArtists[i].id%>"><img src="<%=allArtists[i].imageurl%>" alt="artistImage"></a>
            </div>
            <div class="itemName">
                <div class="artistName">
                    <a class="infoLink" href="/artist/<%=allArtists[i].id%>"><%=allArtists[i].firstname + " "+allArtists[i].lastname%></a>
                </div>
            </div>

        </div>
    <%}%>
</div>